{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ScannerSettings": {
    "ThumbnailsFolder": "thumbnails",
    "PlaceholderThumbnail": "/thumbnails/placeholder.png",
